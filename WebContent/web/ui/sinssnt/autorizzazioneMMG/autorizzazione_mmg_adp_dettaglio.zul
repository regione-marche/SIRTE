				</groupbox>
			</hlayout>
			
			<hlayout width="100%" style="text-align:left;margin:1px"> 
				<groupbox hflex="100" closable="false">
					<caption>
						<div> 
							<label
								value="${c:l('autorizzazionemmg.adp.dettaglio.titolo.gravi.pat')}">
							</label>
