						<div> 
							<label
								value="${c:l('autorizzazionemmg.adp.dettaglio.titolo.deambulante')}">
							</label>
						</div>
					</caption>
					<hlayout width="100%" style="text-align:left">
						<caribelcheckbox hflex="4%" db_name="skadp_attiv1" value="N" />
