<?xml version="1.0" encoding="utf-8"?>
<fo:root xmlns:fo="http://www.w3.org/1999/XSL/Format">
	<!-- mfo:section.start="layout"   -->
	<fo:layout-master-set>
		<fo:simple-page-master master-name="simple" page-height="20cm"
			page-width="29.7cm" margin-top="0.5cm" margin-bottom="0.5cm"
			margin-left="0.5cm" margin-right="0.5cm">
			<fo:region-body margin-top="2.5cm" margin-bottom="0.5cm" />
			<fo:region-before extent="2.5cm" />
			<fo:region-after extent="1cm" />
		</fo:simple-page-master>
	</fo:layout-master-set>
	<fo:page-sequence master-name="simple">
		<fo:static-content flow-name="xsl-region-before">
			<fo:table font-size="10pt" width="auto">
				<fo:table-column column-width="20cm" />
				<fo:table-column column-width="8cm" />
				<fo:table-body>
					<fo:table-row height="10pt">
						<fo:table-cell text-align="start">
							<fo:block start-indent="1mm" end-indent="1mm" padding-top="0.5mm">
							</fo:block>
						</fo:table-cell>
						<fo:table-cell text-align="end">
							<fo:block start-indent="1mm" end-indent="1mm" padding-top="0.5mm">
								<fo:table border="0.5pt solid black" font-size="10pt"
									width="auto">
									<fo:table-column column-width="8cm" />
									<fo:table-body>
										<fo:table-row height="10pt">
											<fo:table-cell text-align="start">
												<fo:block start-indent="1mm" end-indent="1mm"
													padding-top="0.5mm"> 
												Numero cartella #numerocart#
												</fo:block>
											</fo:table-cell>
										</fo:table-row>
										<fo:table-row height="10pt">
											<fo:table-cell text-align="start">
												<fo:block start-indent="1mm" end-indent="1mm"
													padding-top="0.5mm">
												#cognome# #nome#
												</fo:block>
											</fo:table-cell>
										</fo:table-row>
										<fo:table-row height="10pt">
											<fo:table-cell text-align="start">
												<fo:block start-indent="1mm" end-indent="1mm"
													padding-top="0.5mm">
												Data di nascita #datnasc#
												</fo:block>
											</fo:table-cell>
										</fo:table-row>
										<fo:table-row height="10pt">
											<fo:table-cell text-align="start">
												<fo:block start-indent="1mm" end-indent="1mm"
													padding-top="0.5mm">
												Codice fiscale #codfis#
												</fo:block>
											</fo:table-cell>
										</fo:table-row>
									</fo:table-body>
								</fo:table>
							</fo:block>
						</fo:table-cell>
					</fo:table-row>
				</fo:table-body>
			</fo:table>
		</fo:static-content>

		<!-- TITOLO DI PIEDE -->
		<fo:static-content flow-name="xsl-region-after">
			<fo:table font-size="8pt" width="auto" space-before.optimum="5mm">
				<fo:table-column column-width="22cm" />
				<fo:table-column column-width="5.7cm" />
				<fo:table-body>
					<fo:table-row height="10pt">
						<fo:table-cell text-align="start">
							<fo:block start-indent="1mm" end-indent="1mm" padding-top="0.5mm">
							</fo:block>
						</fo:table-cell>
						<fo:table-cell text-align="end">
							<fo:block start-indent="1mm" end-indent="1mm" padding-top="0.5mm">
							Pag.<fo:page-number />
							</fo:block>
						</fo:table-cell>
					</fo:table-row>
				</fo:table-body>
			</fo:table>
		</fo:static-content>
		<fo:flow flow-name="xsl-region-body">
			<!-- mfo:section.end -->

			<!--mfo:section.start="schvalutazioneNODATI" -->
			<fo:table width="auto" space-before.optimum="5pt">
				<fo:table-column column-width="28cm" />
				<fo:table-body>
					<fo:table-row>
						<fo:table-cell>
							<fo:block text-align="start" font-size="15pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm" font-weight="bold">
							SCHEDA DI VALUTAZIONE SOCIALE
							</fo:block>
						</fo:table-cell>
					</fo:table-row>
					<fo:table-row>
						<fo:table-cell>
							<fo:block text-align="start" font-size="12pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							INDICATORE ADEGUATEZZA CONDIZIONE AMBIENTALE
							</fo:block>
						</fo:table-cell>
					</fo:table-row>
					<fo:table-row>
						<fo:table-cell>
							<fo:block text-align="start" font-size="10pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
			                (Valutazione rete assistenziale + valutazione economica di base +
			                valutazione condizione abitativa)
							</fo:block>
						</fo:table-cell>
					</fo:table-row>
				</fo:table-body>
			</fo:table>

			<fo:table width="auto" space-before.optimum="50pt">
				<fo:table-column column-width="28cm" />
				<fo:table-body>
					<fo:table-row>
						<fo:table-cell>
							<fo:block text-align="start" font-size="15pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm" font-weight="bold">
							NON CI SONO DATI PER LA SCALA ADEGUATEZZA CONDIZIONE AMBIENTALE  
							</fo:block>
						</fo:table-cell>
					</fo:table-row>
				</fo:table-body>
			</fo:table>
			<!-- mfo:section.end -->


			<!--mfo:section.start="schvalutazione" -->
			<fo:table width="auto" space-before.optimum="5pt">
				<fo:table-column column-width="28cm" />
				<fo:table-body>
					<fo:table-row>
						<fo:table-cell>
							<fo:block text-align="start" font-size="15pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm" font-weight="bold">
							SCHEDA DI VALUTAZIONE SOCIALE
							</fo:block>
						</fo:table-cell>
					</fo:table-row>
					<fo:table-row>
						<fo:table-cell>
							<fo:block text-align="start" font-size="12pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							INDICATORE ADEGUATEZZA CONDIZIONE AMBIENTALE
							</fo:block>
						</fo:table-cell>
					</fo:table-row>
					<fo:table-row>
						<fo:table-cell>
							<fo:block text-align="start" font-size="10pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
			                (Valutazione rete assistenziale + valutazione economica di base +
			                valutazione condizione abitativa)
							</fo:block>
						</fo:table-cell>
					</fo:table-row>
				</fo:table-body>
			</fo:table>

			<fo:table width="auto" table-omit-header-at-break="false">
				<fo:table-column column-width="8cm" />
				<fo:table-column column-width="1.2cm" />
				<fo:table-column column-width="4.5cm" />
				<fo:table-column column-width="3.0cm" />
				<fo:table-column column-width="5.3cm" />
				<fo:table-column column-width="3.0cm" />
				<fo:table-column column-width="3.0cm" />
				<fo:table-body>
					<fo:table-row>
						<fo:table-cell number-columns-spanned="7" border-style="solid"
							border-width="0.5pt" border-color="rgb(200,200,200)">
							<fo:block text-align="start" font-size="10pt" start-indent="1mm"
								font-weight="bold" end-indent="1mm" padding-top="0.5mm">
							VALUTAZIONE DELLA RETE ASSISTENZIALE
							</fo:block>
						</fo:table-cell>
					</fo:table-row>
					<fo:table-row border-style="solid" border="0.5pt solid black">
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="center" font-size="9pt" start-indent="1mm"
								font-weight="bold" end-indent="1mm" padding-top="0.5mm">
							A
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="center" font-size="9pt" start-indent="1mm"
								font-weight="bold" end-indent="1mm" padding-top="0.5mm">
							B
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="center" font-size="9pt" start-indent="1mm"
								font-weight="bold" end-indent="1mm" padding-top="0.5mm">
							C
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="center" font-size="9pt" start-indent="1mm"
								font-weight="bold" end-indent="1mm" padding-top="0.5mm">
							D
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="center" font-size="9pt" start-indent="1mm"
								font-weight="bold" end-indent="1mm" padding-top="0.5mm">
							E
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="center" font-size="9pt" start-indent="1mm"
								font-weight="bold" end-indent="1mm" padding-top="0.5mm">
							F
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="center" font-size="9pt" start-indent="1mm"
								font-weight="bold" end-indent="1mm" padding-top="0.5mm">
							G
							</fo:block>
						</fo:table-cell>
					</fo:table-row>

					<fo:table-row border-style="solid" border="0.5pt solid black">
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="start" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							Cognome e nome
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="start" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							Anno nascita
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="start" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							Rapporto di parentela o tipo relazione
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="start" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							Convivente
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="start" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							Residenza: specificare comune/provincia o altro Stato
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="center" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							Organizzatore responsabile assistenza (soggetto)
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="center" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							Attuatore interventi (risorsa)
							</fo:block>
						</fo:table-cell>
					</fo:table-row>
					<!--mfo:section.end -->

					<!--mfo:section.start="schvalutazioneCrp" -->
					<fo:table-row height="10pt" border-style="solid"
						border="0.5pt solid black">
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="start" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							#Cognome#
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="center" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							#nascita#
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
