							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="center" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							#conv#
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="start" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							#Comunedes#
							</fo:block>
							<fo:block text-align="start" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							#numtel#
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="end" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							#respassist#
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="end" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							#attuainterv#
							</fo:block>
						</fo:table-cell>
					</fo:table-row>
					<!--mfo:section.end -->


					<!--mfo:section.start="schvalutazioneCrpInt" -->
					<fo:table-row height="10pt" border-style="solid"
						border="0.5pt solid black">
						<fo:table-cell number-columns-spanned="5" border-style="solid"
							border-width="0.5pt" border-color="rgb(200,200,200)"
							background-color="rgb(200,200,200)">
							<fo:block text-align="start" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
			                #descrizione#
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="end" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
			                #assitito#
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)"
							background-color="rgb(200,200,200)">
							<fo:block text-align="center" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							</fo:block>
						</fo:table-cell>
					</fo:table-row>
					<!--mfo:section.end -->

					<!--mfo:section.start="schvalutazioneCrpConNoc" -->
					<fo:table-row height="10pt" border-style="solid"
						border="0.5pt solid black" background-color="rgb(200,200,200)">
						<fo:table-cell number-columns-spanned="7" border-style="solid"
							border-width="0.5pt" border-color="rgb(200,200,200)">
							<fo:block text-align="start" font-size="9pt" start-indent="1mm"
								font-weight="bold" end-indent="1mm" padding-top="0.5mm">
							#TipoConv#
							</fo:block>
						</fo:table-cell>
					</fo:table-row>
					<!--mfo:section.end -->

					<!--mfo:section.start="schvalutazioneIacad" -->
					<fo:table-row>
						<fo:table-cell number-columns-spanned="6" border-width="0.5pt"
							border-color="rgb(200,200,200)" border-left-style="solid"
							border-bottom-style="solid">
							<fo:block text-align="start" font-size="10pt" start-indent="1mm"
								font-weight="bold" end-indent="1mm" padding-top="0.5mm">
							PUNTEGGIO PARZIALE per IACAd (sommatoria colonna G: pt 0-20)
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-width="0.5pt"
							border-color="rgb(200,200,200)" border-left-style="solid"
							border-right-style="solid" border-bottom-style="solid">
							<fo:block text-align="center" font-size="8pt" start-indent="1mm"
								font-weight="bold" end-indent="1mm" padding-top="0.5mm">
							#puntIACAD#/20
							</fo:block>
						</fo:table-cell>
					</fo:table-row>
					<!--mfo:section.end -->

					<!--mfo:section.start="schvalutazioneEnd" -->
					<fo:table-row border-style="solid" border="0.5pt solid black">
						<fo:table-cell number-columns-spanned="5">
							<fo:block text-align="end" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							PUNTEGGI
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="center" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							#totrespassist#
							</fo:block>
						</fo:table-cell>
						<fo:table-cell border-style="solid" border-width="0.5pt"
							border-color="rgb(200,200,200)">
							<fo:block text-align="center" font-size="9pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							#totattuainterv#
							</fo:block>
						</fo:table-cell>
					</fo:table-row>

					<!-- spaziatura -->
					<fo:table-row>
						<fo:table-cell number-columns-spanned="7">
							<fo:block text-align="start" font-size="8pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							</fo:block>
						</fo:table-cell>
					</fo:table-row>
					<fo:table-row>
						<fo:table-cell number-columns-spanned="7">
							<fo:block text-align="start" font-size="8pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
							</fo:block>
						</fo:table-cell>
