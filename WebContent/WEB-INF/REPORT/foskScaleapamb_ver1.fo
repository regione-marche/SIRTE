							<fo:block text-align="start" font-size="10pt" start-indent="1mm"
								font-weight="bold" end-indent="1mm" padding-top="0.5mm">
							Indicatore Rete Assistenziale:
							</fo:block>
						</fo:table-cell>
						<fo:table-cell number-columns-spanned="2" border-width="0.5pt"
							border-color="rgb(200,200,200)" border-top-style="solid"
							border-left-style="solid" border-right-style="solid">
							<fo:block text-align="start" font-size="8pt" start-indent="1mm"
								font-weight="bold" end-indent="1mm" padding-top="0.5mm">
							</fo:block>
						</fo:table-cell>
					</fo:table-row>

					<fo:table-row>
						<fo:table-cell number-columns-spanned="5" border-width="0.5pt"
							border-color="rgb(200,200,200)" border-left-style="solid"
							border-bottom-style="solid">
							<fo:block text-align="start" font-size="10pt" start-indent="1mm"
								font-weight="bold" end-indent="1mm" padding-top="0.5mm">
							PUNTEGGIO TOTALE (sommatoria colonne F+G)
							</fo:block>
						</fo:table-cell>
						<fo:table-cell number-columns-spanned="2" border-width="0.5pt"
							border-color="rgb(200,200,200)" border-left-style="solid"
							border-right-style="solid" border-bottom-style="solid">
							<fo:block text-align="center" font-size="8pt" start-indent="1mm"
								font-weight="bold" end-indent="1mm" padding-top="0.5mm">
							#punt#/20
							</fo:block>
						</fo:table-cell>
					</fo:table-row>

					<!-- spaziatura -->
					<fo:table-row>
						<fo:table-cell number-columns-spanned="7">
							<fo:block text-align="start" font-size="8pt" start-indent="1mm"
								end-indent="1mm" padding-top="0.5mm">
