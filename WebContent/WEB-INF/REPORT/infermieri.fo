         <fo:table-column column-width="2cm"/>
         <fo:table-column column-width="2cm"/>
        <fo:table-body>
          <fo:table-row  height="10pt">
            <fo:table-cell>
              <fo:block text-align="start" font-weight="bold" font-size="10pt"
